#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/superatom/WeChatProjects/miniprogram-1/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules/.pnpm/typescript@3.9.10/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules/.pnpm/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules:/Users/superatom/WeChatProjects/node_modules:/Users/superatom/node_modules:/Users/node_modules:/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules/typescript/bin/node_modules:/Users/superatom/WeChatProjects/miniprogram-1/node_modules/typescript/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
